{
  "name": "hexo-theme-keep-starter",
  "version": "3.6.0",
  "private": false,
  "scripts": {
    "themes": "rm -rf ./themes/keep/source/images && mkdir ./themes/keep/source/images &&cp -r ./source/_images/.  ./themes/keep/source/images &&mkdir ./themes/keep/source/images/papers &&cp -r `find ./source/_posts/ -mindepth 1 -type d` ./themes/keep/source/images/papers",
    "server": "hexo clean &&npm run themes && hexo server --open",
    "build": "hexo clean && hexo generate",
    "clean": "hexo clean",
    "deploy": "hexo clean &&npm run themes && hexo deploy"
  },
  "author": "XPoet",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:theme-keep/hexo-theme-keep-starter.git"
  },
  "bugs": {
    "url": "https://github.com/theme-keep/hexo-theme-keep-starter/issues"
  },
  "homepage": "https://github.com/theme-keep/hexo-theme-keep-starter#readme",
  "hexo": {
    "version": "6.3.0"
  },
  "dependencies": {
    "hexo": "^6.3.0",
    "hexo-deployer-git": "^4.0.0",
    "hexo-generator-archive": "^2.0.0",
    "hexo-generator-category": "^2.0.0",
    "hexo-generator-index": "^3.0.0",
    "hexo-generator-searchdb": "^1.4.1",
    "hexo-generator-tag": "^2.0.0",
    "hexo-reference": "^1.0.4",
    "hexo-renderer-ejs": "^2.0.0",
    "hexo-renderer-marked": "^6.0.0",
    "hexo-renderer-stylus": "^2.1.0",
    "hexo-server": "^3.0.0",
    "hexo-theme-keep": "^3.6.0",
    "hexo-wordcount": "^6.0.1"
  }
}
